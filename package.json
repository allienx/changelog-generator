  "homepage": "https://github.com/allienx/actions-changelog-generator#readme",
  "bugs": {
    "url": "https://github.com/allienx/actions-changelog-generator/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/allienx/actions-changelog-generator.git"
  },
  "license": "ISC",
  "author": "Alex Lin",
  "scripts": {
