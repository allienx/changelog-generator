  "dependencies": {
    "@actions/core": "3.0.0",
    "@actions/github": "9.0.0"
  },
  "devDependencies": {
    "@octokit/rest": "22.0.1",
    "@octokit/types": "16.0.0",
    "@types/node": "25.2.3",
    "@vercel/ncc": "0.38.4",
    "prettier": "3.8.1",
    "tsx": "4.21.0",
    "typescript": "5.9.3"
  }
